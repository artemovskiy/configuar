{
  "name": "configuar",
  "version": "0.3.0-alpha.0",
  "description": "Transform to TS and validate environment variables",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "dotenv",
    "env",
    "envenvironment variables",
    "config",
    "settings",
    "typescript",
    "decorator",
    "validation"
  ],
  "bin": {
    "configuar": "./bin/cli.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "lint": "eslint \"{src,test}/**/*.ts\"",
    "test": "jest",
    "test:ci": "jest --coverage --ci -c ci.jest.config.js",
    "test:e2e": "jest -c ./test/e2e.jest.config.js",
    "prepare": "husky install"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/json-schema": "^7.0.11",
    "ajv": "^8.12.0",
    "commander": "^10.0.0",
    "reflect-metadata": "^0.1.13",
    "typereader": "^0.0.3"
  },
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@types/mock-fs": "^4.13.1",
    "@types/node": "^16.10.2",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "eslint": "^7.11.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.4",
    "eslint-plugin-import": "^2.25.4",
    "husky": "^6.0.0",
    "jest": "^28.1.3",
    "jest-junit": "^12.0.0",
    "lint-staged": "^11.0.0",
    "mock-fs": "^5.1.4",
    "prettier": "^2.2.1",
    "rimraf": "^2.5.4",
    "ts-jest": "^28.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.4.3"
  },
  "engines": {
    "node": ">= 12"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xydens/configuar"
  }
